{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Issuer": "rohit",
    "Secret": "super-secret-key",
    "ExpiryMinutes": 60,
    "Audience": "rohit"
  },
  "TokenBucketSettings": {
    "RedisConnectionString": "localhost",
    "KeyPrefix": "rate_limit_client",
    "MaxTokens": 11,
    "KeyPrefixIp": "rate_limit_ip",
    "MaxTokensIp": 50,
    "KeyPrefixGlobal": "rate_limit_global",
    "MaxTokensGlobal": 200,
    "RefillRate": 1
  }
}
